  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@types/axios": "^0.14.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "typescript": "^5.3.3"
  }
}
