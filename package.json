{
  "name": "web-search",
  "version": "0.1.0",
  "description": "web search the internet",
  "private": true,
  "type": "module",
  "bin": {
    "web-search": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
